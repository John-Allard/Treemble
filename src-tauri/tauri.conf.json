{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "Treemble",
  "version": "1.2.0",
  "identifier": "com.johnallard.treemble",
  "build": {
    "frontendDist": "../dist",
    "devUrl": "http://localhost:1420",
    "beforeDevCommand": "",
    "beforeBuildCommand": ""
  },
  "app": {
    "windows": [
      {
        "title": "Treemble",
        "width": 1100,
        "height": 900,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "assetProtocol": {
        "enable": true,
        "scope": ["$HOME/**"]
      },
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": ["deb", "msi", "app"],
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "windows": {
      "certificateThumbprint": null,
      "digestAlgorithm": "sha256",
      "timestampUrl": null
    },
    "macOS": {
      "signingIdentity": null
    }
  }
}
